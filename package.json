{
  "name": "vite-vue2-unhead-ssr-ts-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "serve:dev": "cross-env NODE_ENV=development vite serve",
    "ssr:dev": "cross-env NODE_ENV=development node --inspect ./src/server.js",
    "ssr:prod": "cross-env NODE_ENV=production node ./src/server.js",
    "build:manifest:dev": "vite build --ssrManifest ssr-manifest.json --mode development --outDir ../dist",
    "build:manifest:prod": "vite build --ssrManifest ssr-manifest.json --mode production --outDir ../dist",
    "build:ssr:dev": "vite build --ssr ./entry-server.ts --mode development --outDir ../dist-server",
    "build:ssr:prod": "vite build --ssr ./entry-server.ts --mode production --outDir ../dist-server",
    "build:dev": "yarn build:manifest:dev && yarn build:ssr:dev",
    "build:prod": "yarn build:manifest:prod && yarn build:ssr:prod"
  },
  "dependencies": {
    "@unhead/ssr": "^1.8.7",
    "@unhead/vue": "^1.8.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "ofetch": "^1.3.3",
    "pinia": "^2.1.7",
    "reflect-metadata": "^0.1.14",
    "typeorm": "^0.3.17",
    "unhead": "^1.8.7",
    "vue": "^2.7.15",
    "vue-cookies": "^1.8.3",
    "vue-router": "^3.6.5",
    "vuetify": "^2.7.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/express": "types/express",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.17.1",
    "@vitejs/plugin-legacy": "^4.0.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vitejs/plugin-vue2-jsx": "^1.1.0",
    "@vue/tsconfig": "^0.1.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "sass": "~1.32.13",
    "serve-static": "^1.15.0",
    "terser": "^5.24.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.17.2",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-ssr": "^0.4.142",
    "vue-server-renderer": "^2.7.15"
  }
}
